{{!-- Picks every post from this category and renders to page --}}
<div class="container">
    {{#each post}}
    <div class="card">
        <div class="card-header">
           <a href="/{{category}}/{{id}}"> Title: {{header}}</a> 
        </div>
        <div class="body">
            {{corntent}}
        </div>
    </div>
    {{/each}}
</div>